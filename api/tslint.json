{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "// .*"
      }
    ],
    "prefer-for-of": false,
    "no-shadowed-variable": false,
    "variable-name": false,
    "no-string-literal": false
  },
  "rulesDirectory": []
}
